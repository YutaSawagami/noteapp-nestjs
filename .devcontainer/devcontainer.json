{
    "name": "noteapp-nestjs",
    "build": {
      "dockerfile": "../Dockerfile",
      "context": "..",
      "target": "base"
    },
    "mounts": [
      "source=memo-nestjs_nodemodules,target=${containerWorkspaceFolder}/node_modules,type=volume"
    ],
    "customizations": {
      "vscode": {
        "extensions": [
          "esbenp.prettier-vscode",
          "christian-kohler.npm-intellisense"
        ]
      }
    },
    "postCreateCommand": "bash -x .devcontainer/postCreateCommand.sh"
  }
  